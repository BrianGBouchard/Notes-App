# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'NotesApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NotesApp

  pod 'Firebase'
  pod 'FirebaseCore'
  pod 'FirebaseAuth'
  pod 'FirebaseDatabase'
  pod 'RNCryptor', '~> 5.0'
  pod 'LUKeychainAccess'


end
